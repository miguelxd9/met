# Configuración de SonarCloud API
SONARCLOUD_TOKEN=your_sonarcloud_token_here
SONARCLOUD_ORGANIZATION=your_organization_key_here

# Configuración de Base de Datos
DATABASE_URL=postgresql://postgres:password@localhost:5432/sonarcloud_metrics

# Configuración de API
API_BASE_URL=https://sonarcloud.io/api
API_RATE_LIMIT=1000
API_TIMEOUT=30
API_RETRY_ATTEMPTS=1

# Configuración de Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE=logs/sonarcloud_metrics.log

# Configuración de Métricas
METRICS_COLLECTION_INTERVAL=3600
BATCH_SIZE=100
