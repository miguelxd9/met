# Configuración de Bitbucket API
BITBUCKET_USERNAME=tu_usuario_bitbucket
BITBUCKET_APP_PASSWORD=tu_app_password_bitbucket
BITBUCKET_WORKSPACE=tu_workspace_slug

# Configuración de Base de Datos PostgreSQL
DATABASE_URL=postgresql://usuario:password@localhost:5432/bitbucket_metrics

# Configuración de API
API_BASE_URL=https://api.bitbucket.org/2.0
API_RATE_LIMIT=1000
API_TIMEOUT=30
API_RETRY_ATTEMPTS=3

# Configuración de Logging
LOG_LEVEL=INFO
LOG_FORMAT=console
LOG_FILE=logs/bitbucket_metrics.log

# Configuración de Métricas
METRICS_COLLECTION_INTERVAL=3600
BATCH_SIZE=100
