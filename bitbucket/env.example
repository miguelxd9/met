# Configuración de Bitbucket API
BITBUCKET_USERNAME=your_bitbucket_username
BITBUCKET_APP_PASSWORD=your_bitbucket_app_password
BITBUCKET_WORKSPACE=abc

# Configuración de SonarCloud API
SONARCLOUD_TOKEN=your_sonarcloud_token
SONARCLOUD_ORGANIZATION=abc
SONARCLOUD_API_BASE_URL=https://sonarcloud.io/api

# Configuración de Base de Datos SQL Server Azure
DATABASE_URL=mssql+pyodbc:///?odbc_connect=Driver={ODBC Driver 18 for SQL Server};Server=tcp:abc.database.windows.net,1433;Database=devops_metrics;Uid=={your_uid_here};Pwd={your_password_here};Encrypt=yes;TrustServerCertificate=no;Connection Timeout=30;

# Configuración de API
API_BASE_URL=https://api.bitbucket.org/2.0
API_RATE_LIMIT=1000
API_TIMEOUT=30
API_RETRY_ATTEMPTS=1

# Configuración de Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE=logs/bitbucket_metrics.log

# Configuración de Métricas
METRICS_COLLECTION_INTERVAL=3600
BATCH_SIZE=100
